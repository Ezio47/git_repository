<script type="text/javascript">
    function Manager() {
        var dic305Value = $('#dic305Value').val().split(',');
        var dic305Count = $('#dic305Count').val();
        if (dic305Count != "") {
            dic305Count = parseInt(dic305Count);
        }
        var dic311Value = $('#dic311Value').val().split(',');
        var dic311Count = $('#dic311Count').val();
        if (dic311Count != "") {
            dic311Count = parseInt(dic311Count);
        }
        var strREPORTCODE = "";
        var strSSXTYPELEVELCODE = "";
        var strREPORTVALUE = "";
        for (var x = 0; x < dic305Count; x++) {
            for (var y = 0; y < dic311Count-1; y++) {
                var temp = x.toString() + y.toString();
                var s = $('#tbx' + temp + '').val();
                if ($.trim(s) != "") {
                    if (!CheckNUM2(s)) {
                        layer.msg('请输入整数!', { icon: 5 });
                        $('#tbx' + temp + '').focus();
                        return;
                    }
                    else {
                        if (s == "0" || s == "") {
                            continue;
                        }
                        else {
                            strREPORTVALUE = strREPORTVALUE + $('#tbx' + temp + '').val() + ",";
                            strSSXTYPELEVELCODE = strSSXTYPELEVELCODE + dic305Value[x] + ",";
                            strREPORTCODE = strREPORTCODE + dic311Value[y] + ",";
                            //if (y == dic311Count - 1) {
                            //    strREPORTVALUE = strREPORTVALUE + $('#tbxREPORTVALUE').val() + ",";
                            //}
                        }
                    }
                }
            }
        }
        $.ajax({
            url: '/FIRERECORD/FIRERECORD_REPORT9Manager',
            type: 'post',
            cache: false,
            timeout: 10000, //10秒后超时，如果为0则永不超时
            data: {
                REPORTYEAR: $("#tbxREPORTYEAR").val(),
                BYORGNO: $("#tbxBYORGNO").val(),
                REPORTCODE: strREPORTCODE,
                SSXTYPELEVELCODE: strSSXTYPELEVELCODE,
                REPORTVALUE: strREPORTVALUE,
                Method: '@ViewBag.Method',
            },
            success: function (data, ioArgs) {
                var ar = eval('(' + data + ')');
                if (ar.Success) {
                    if (confirm(ar.Msg + ' 请返回?')) {
                        var index = parent.layer.getFrameIndex(window.name); //获取窗口索引
                        parent.layer.close(index);
                    }
                }
                else {
                    layer.msg(ar.Msg, { icon: 5 });
                }
            },
            error: function (err, ioArgs) {
                layer.msg('系统异常!', { icon: 5 });
            }
        });
    }
</script>
