    //获取热点类型信息
    function loadClass() {
        $.ajax({
            url: '/MainDefault/GetHotInfoJson',
            data: {},
            type: 'post',
            success: function (data, ioArgs) {
                var ar = eval('(' + data + ')');
                if (ar.Success) {
                    $('#divwarnPanelBody').html(ar.Msg);
                    $('.hqjc>.floatRight').css("margin-top", "23px");
                }
                else {
                    layer.msg("获取错误!");
                }
            },
            error: function (err, ioArgs) {
