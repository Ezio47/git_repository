    //获取在线系统人员
    function loadLineClass() {
        $.ajax({
            url: '/MainDefault/GetLineInfoJson',
            data: {},
            type: 'post',
            success: function (data, ioArgs) {
                var ar = eval('(' + data + ')');
                if (ar.Success) {
                    $('#divGetLineInfo').html(ar.Msg);
                }
                else {
                    layer.msg("获取错误!");
                }
            },
            error: function (err, ioArgs) {
                layer.msg("系统错误!")
            }
        });
