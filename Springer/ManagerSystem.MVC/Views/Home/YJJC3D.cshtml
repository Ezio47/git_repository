<script>
    $(function () {
        loadClass();
        loadLineClass();
    });

    //用户在线、不在线情况
    var str = "";
    function showPerson(obj) {
        if (obj == "0") {
            str = "离线人员信息";
        }
        else {
            str = "在线人员信息";
        }
        qrueyHTML_showPerson(obj, str);
    }
    //护林员
    function showHlyPerson(obj) {
        var titlestr = "";
        if (obj == "0") {
            titlestr = "在线护林员信息";
        }
        else if (obj == "1") {
