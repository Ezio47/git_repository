}


<div>
    <img src="~/Images/img/login_img.png" class="login_left" />
</div>
<div class="login_right">
    <div class="loginForm">
        <div class="logo"></div>
        <div class="welcome">欢迎登陆</div>
        <div class="loginInput ">
            <input type="text" value="admin" placeholder="请输入用户名" />
            <div class="icon user"></div>
        </div>
        <div class="loginInput ">
            <input type="password" value="123456" placeholder="请输入密码" />
            <div class="icon pwd"></div>
        </div>
        <label class="checklabel">
            <input type="checkbox" />
            <span>记住密码</span>
        </label>
        <div class="loginBtns">
            <a onclick="Login()">登录</a>
            <a href="#">重置</a>
        </div>
